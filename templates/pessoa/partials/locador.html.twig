<div class="card mb-4">
    <div class="card-header">
        <h3><i class="fas fa-key"></i> Dados do Locador</h3>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-md-4">{{ form_row(form.locador.formaRetirada) }}</div>
            <div class="col-md-4">{{ form_row(form.locador.dependentes) }}</div>
            <div class="col-md-4">{{ form_row(form.locador.diaRetirada) }}</div>
        </div>
